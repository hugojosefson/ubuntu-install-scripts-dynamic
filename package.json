{
  "name": "ubuntu-install-scripts",
  "version": "0.0.1-alpha",
  "description": "Install scripts for various things I like to install on a fresh Ubuntu",
  "main": "index.mjs",
  "type": "module",
  "scripts": {
    "start": "node index.mjs",
    "docker-build": "docker build -t hugojosefson/ubuntu-install-scripts-dynamic .",
    "docker-run": "docker run hugojosefson/ubuntu-install-scripts-dynamic",
    "lint": "prettier --write '{src,test}/**/*.{js,mjs,md}' './*.{js,mjs,md}'",
    "test": "npm-run-all lint mocha",
    "mocha": "mocha"
  },
  "author": "Hugo Josefson <hugo@josefson.org> (https://www.hugojosefson.com/)",
  "repository": "hugojosefson/ubuntu-install-scripts-dynamic",
  "license": "MIT",
  "keywords": [
    "ubuntu",
    "install",
    "scripts"
  ],
  "bugs": {
    "url": "https://github.com/hugojosefson/ubuntu-install-scripts-dynamic/issues"
  },
  "homepage": "https://github.com/hugojosefson/ubuntu-install-scripts-dynamic",
  "mocha": {
    "spec": "test/**/*test.mjs"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{mjs,js,md}": [
      "yarn lint"
    ]
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2",
    "mocha": "^8.1.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5"
  },
  "dependencies": {
    "debounce": "^1.0.0",
    "faithful-exec": "^0.1.0",
    "inpath": "^1.0.2",
    "is-root": "^2.1.0",
    "lodash": "^4.17.19",
    "minimist": "^1.2.5",
    "sudo": "^1.0.3"
  },
  "engines": {
    "node": ">=13.2.0"
  }
}
